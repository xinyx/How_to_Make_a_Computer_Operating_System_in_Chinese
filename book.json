        "sharing": {
            "google"   : true,
            "facebook" : true,
            "twitter"  : true,
            "weibo"    : true,
            "qq"       : true,
            "qrcode"   : true
        }
    },
    "pdf": {
        "toc": true,
        "pageNumbers": true,
        "fontSize": 12,
