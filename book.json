    },
    "plugins": [
        "-mathjax", "ga", "duoshuo"
    ],
    "pluginsConfig": {
        "fontSettings": {
            "theme": "white",
            "family": "serif",
            "size": 2
        },
        "ga": {
            "token": "UA-39088953-8"
        },
